    var bCurrentBranchCookie = document.cookie.match(/CurrentBranch=[^;]+/);

        function querystring(key) {
            var re = new RegExp('(?:\\?|&)' + key + '=(.*?)(?=&|$)', 'gi');
            var r = [], m;
            while ((m = re.exec(document.location.search)) != null) r.push(m[1]);
            return r;
           }

        if (bCurrentBranchCookie || (querystring("instorecode") != "" && querystring("instorecode") != [])) {
            var oHeadElement = document.getElementsByTagName('head')[0];
            
            if (oHeadElement) {
                var aLinkElements = document.getElementsByTagName('link');

                for (var i = 0; i < aLinkElements.length; i++) {
                    var oLinkElement = aLinkElements[i];

                    if (oLinkElement.href && oLinkElement.href.indexOf('css/styles') > -1) {
                        oHeadElement.removeChild(oLinkElement);

                        var oNewLinkElement = document.createElement('link');
                        oNewLinkElement.type = 'text/css';
                        oNewLinkElement.rel = 'stylesheet';
                        oNewLinkElement.href = _root + 'css/styles-storecolumn-min.css';

                        oHeadElement.appendChild(oNewLinkElement);
                        
                        break;
                    }
                }
            }
        }
