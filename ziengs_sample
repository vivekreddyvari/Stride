         <div class="bttnWrap">
          <div class="title">
           <div id="countDown">
            Bestel binnen
            <div id="deliveryCountDown">
            </div>
            <strong>
             morgen in huis
            </strong>
           </div>
           <script type="text/javascript">
            var orderDate = new Date(1464717600000);
	var secDef = 1000; 
	var minDef = 60 * secDef;
	var hourDef = 60 * minDef;
	var dayDef = 24 * hourDef;

	$(function () {		
		OrderCountDown();		
	});	

	function OrderCountDown() {
		setTimeout(function () {			
			var thisDate = new Date();
			var daysLeft = parseInt((orderDate - thisDate) / dayDef);
			var hoursLeft = parseInt((orderDate - thisDate) / hourDef);
			var minutesLeft = parseInt((orderDate - thisDate) / minDef);
			var secondsLeft = parseInt((orderDate - thisDate) / secDef);
			
			seconds = secondsLeft - (minutesLeft * 60);			
			minutes = minutesLeft - (hoursLeft * 60);			
			hours = hoursLeft - (daysLeft * 24);			
			ShowCount(hours, minutes, seconds);

		}, 1000);
	}

	function ShowCount(hours, minutes, seconds) {		
        $('#deliveryCountDown').html(hours + "u " + minutes + "min " + seconds + "sec");
		if(!(hours == 0 && minutes == 0 && seconds == 0 || seconds < 0)){
			OrderCountDown();
		}else{
		    $('#countDown').html('Gratis NL verzending &amp; retourneren');
		}
	}
           </script>
